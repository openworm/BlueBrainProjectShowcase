        <Constant name = "VOLT_SCALE" value = "1 mV" dimension = "voltage"/>
        <Dynamics>
            <DerivedVariable name="r" dimension="per_time" exposure="r" value="0.11325*exp(0.025*V) / TIME_SCALE"/>
            <DerivedVariable name="V" dimension="none" value="v / VOLT_SCALE"/>
        </Dynamics>
    </ComponentType>


